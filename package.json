    "react-bootstrap": "^1.0.0-beta.16",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-player": "^1.12.0",
    "react-redux": "^7.1.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "reactstrap": "^8.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.1",
